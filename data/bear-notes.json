{
  "vars": {
    "weekNum": "7",
    "weekDays": "9-15feb2026",
    "pastWeekNum": "6",
    "pastWeekDays": "2-8feb2026"
  },
  "mods": ["ctrl", "alt", "shift", "cmd"],
  "notes": [
    {"key": "N", "action": "open-bear-note", "title": "_mem NOW"},
    {"key": "R", "action": "open-bear-note", "title": "_app rcmd"},
    {"key": "D", "action": "open-bear-note", "title": "w${weekNum}days ${weekDays}", "pastTitle": "w${pastWeekNum}days ${pastWeekDays}"},
    {"key": "W", "action": "open-bear-note", "title": "w${weekNum}work ${weekDays}", "pastTitle": "w${pastWeekNum}work ${pastWeekDays}"},
    {"key": "T", "action": "open-bear-note", "title": "w${weekNum}thoughts ${weekDays}", "pastTitle": "w${pastWeekNum}thoughts ${pastWeekDays}"},
    {"key": "S", "action": "open-bear-note", "title": "_topsight 2026"},
    {"key": "I", "action": "open-bear-note", "title": "_index 2026"}
  ]
}
