{
  "vars": {
    "weekNum": "6",
    "weekDays": "2-8feb2026"
  },
  "mods": ["ctrl", "alt", "shift", "cmd"],
  "notes": [
    {"key": "N", "action": "open-bear-note", "title": "_mem NOW"},
    {"key": "R", "action": "open-bear-note", "title": "_app rcmd"},
    {"key": "D", "action": "open-bear-note", "title": "w${weekNum}days ${weekDays}"},
    {"key": "W", "action": "open-bear-note", "title": "w${weekNum}work ${weekDays}"},
    {"key": "T", "action": "open-bear-note", "title": "w${weekNum}thoughts ${weekDays}"},
    {"key": "S", "action": "open-bear-note", "title": "_topsight 2026"},
    {"key": "I", "action": "open-bear-note", "title": "_index 2026"}
  ]
}
